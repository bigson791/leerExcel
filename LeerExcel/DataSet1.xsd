            <MainSource>
              <DbSource ConnectionRef="ConnectionString (Settings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="getHojasVentana" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="getHojasVentana" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="getHojasVentana">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>WITH OrderedData AS (SELECT   CLIENTE, NO_ORDEN, NO_VENTANA,DESCRIPCION_ARTICULO, NO_CORTE, MEDIDA, POSICION, ID_VENTANA, ROW_NUMBER() OVER(ORDER BY ID_VENTANA) AS row_num, COUNT(*) OVER(PARTITION BY POSICION) 
AS position_count
FROM      ordenes
WHERE   NO_ORDEN = 'A2352-1' AND DESCRIPCION_ARTICULO LIKE '%hoja%'), PositionedData AS
    (SELECT   CLIENTE, NO_ORDEN, NO_VENTANA,DESCRIPCION_ARTICULO, NO_CORTE, MEDIDA, POSICION, ID_VENTANA, row_num, position_count, 
                     CASE WHEN POSICION = 'Horizontal inferior' THEN 1 WHEN POSICION = 'Horizontal superior' THEN 2 WHEN POSICION = 'Vertical izquierda' THEN 3 WHEN POSICION = 'Vertical derecha' THEN 4 ELSE 5 END AS posicion_order
     FROM      OrderedData), GroupedData AS
    (SELECT   CLIENTE, NO_ORDEN, NO_VENTANA, DESCRIPCION_ARTICULO, NO_CORTE, MEDIDA, POSICION, ID_VENTANA, row_num, position_count, posicion_order, FLOOR((row_num - 1) / (4 * position_count)) AS group_num
     FROM      PositionedData), GroupsWithPositionOrder AS
    (SELECT   CLIENTE, NO_ORDEN, NO_VENTANA, DESCRIPCION_ARTICULO, NO_CORTE, MEDIDA, POSICION, ID_VENTANA, row_num, position_count, posicion_order, group_num, ROW_NUMBER() OVER(PARTITION BY POSICION, group_num
     ORDER BY ID_VENTANA) AS row_num_in_group
FROM      GroupedData)
    SELECT   CLIENTE, NO_ORDEN,NO_VENTANA, DESCRIPCION_ARTICULO, NO_CORTE, MEDIDA, POSICION, ID_VENTANA
    FROM      GroupsWithPositionOrder
    ORDER BY group_num, row_num_in_group, posicion_order, ID_VENTANA, DESCRIPCION_ARTICULO;</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
