  <Code>using System;
using System.IO;
using System.Xml;

namespace RDLCColumnWidthAdjustment
{
    class Program
    {
        static void Main(string[] args)
        {
            // Ruta al archivo RDLC (debes ajustarla según tu proyecto)
            string rdlcFilePath = @"C:\Users\DELL\source\repos\LeerExcel\LeerExcel\Report1.rdlc";

            // Cargar el archivo RDLC como XML
            XmlDocument doc = new XmlDocument();
            doc.Load(rdlcFilePath);

            // Namespace para elementos de informe RDLC
            XmlNamespaceManager namespaceManager = new XmlNamespaceManager(doc.NameTable);
            namespaceManager.AddNamespace("r", "http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition");

            // Nombre del elemento de columna a ajustar (debes ajustar esto según tu informe)
            string columnName = "NombreDeTuColumna";

            // Nuevo ancho deseado para la columna (en puntos)
            double newWidth = 420; // Ajusta el valor según tus necesidades

            // Buscar el elemento de columna
            XmlNode columnNode = doc.SelectSingleNode($"//r:TablixColumn[@Name='{columnName}']", namespaceManager);
            if (columnNode != null)
            {
                // Cambiar el ancho de la columna
                XmlAttribute widthAttribute = columnNode.Attributes["Width"];
                if (widthAttribute != null)
                {
                    widthAttribute.Value = $"{newWidth}pt";
                }

                // Guardar los cambios en el archivo RDLC
                doc.Save(rdlcFilePath);
                Console.WriteLine("Ancho de columna ajustado exitosamente.");
            }
            else
            {
                Console.WriteLine("Columna no encontrada en el informe.");
            }
        }
    }
}</Code>
